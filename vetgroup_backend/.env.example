# Copy this file to .env and provide your own secure values before running Strapi locally.

APP_KEYS=change-me-1,change-me-2
ADMIN_JWT_SECRET=change-me-admin
API_TOKEN_SALT=change-me-api
TRANSFER_TOKEN_SALT=change-me-transfer

# Database defaults to sqlite; switch to mysql/postgres by configuring below.
DATABASE_CLIENT=sqlite
DATABASE_FILENAME=.tmp/data.db

# Optional host/port overrides
HOST=0.0.0.0
PORT=1337
